    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "auto_remaster/train_auto_remaster.py",
            "type": "debugpy",
            "request": "launch",
            "module": "accelerate.commands.launch",
            "cwd": "${workspaceFolder}",
            "args": [
                "--config_file=/code/auto_remaster/single_gpu.yaml",
                "--module",
                "auto_remaster.train_auto_remaster",
                "--config",
                "/code/auto_remaster/configs/sd1.5_ddpm.yaml",
            ],
            "justMyCode": false,
            "env": {
                "http_proxy": "127.0.0.1:2334",
                "https_proxy": "127.0.0.1:2334",
                "CUDA_VISIBLE_DEVICES": "0"
            }
        },
        {
            "name": "auto_remaster/train_auto_remaster_lbm.py",
            "type": "debugpy",
            "request": "launch",
            "module": "accelerate.commands.launch",
